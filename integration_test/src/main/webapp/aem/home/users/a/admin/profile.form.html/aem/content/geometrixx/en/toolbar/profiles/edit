  function cq5forms_preCheck_profile(submitid) {
    var dMsgs = "Please fill out the required field.";



if (!cq5forms_isEmpty(document.forms["profile"].elements["age"])){
{var obj =document.forms["profile"].elements["age"];if ( cq5forms_isArray(obj)) {for(i=0;i<obj.length;i++) {if (!cq5forms_regcheck(obj[i].value, /([\d])+/)) {cq5forms_showMsg('profile','age','Field does not contain a number.', i); return false;}}} else {if (!cq5forms_regcheck(obj.value, /([\d])+/)) {cq5forms_showMsg('profile','age','Field does not contain a number.'); return false;}}}
}




if (!cq5forms_isEmpty(document.forms["profile"].elements["email"])){
{var obj =document.forms["profile"].elements["email"];if ( cq5forms_isArray(obj)) {for(i=0;i<obj.length;i++) {if (!cq5forms_regcheck(obj[i].value, /^[^@]+([@]{1})[0-9a-zA-Z\._-]+([\.]{1})[0-9a-zA-Z\._-]+$/)) {cq5forms_showMsg('profile','email','Field does not contain a valid email address.', i); return false;}}} else {if (!cq5forms_regcheck(obj.value, /^[^@]+([@]{1})[0-9a-zA-Z\._-]+([\.]{1})[0-9a-zA-Z\._-]+$/)) {cq5forms_showMsg('profile','email','Field does not contain a valid email address.'); return false;}}}
}



    return true;
  }
</script>
