<div id="nav-wrapper">
    <!-- Nav -->
    <nav id="nav">
        <ul>
            <li class="home"><a href="/index.html">Home</a></li>
            {% for item in site.mainNavItems %}
                <li{% if page.url == item.link %} class="active"{% endif %}><a href="{{item.link}}" title="{{item.title}}">{{item.label}}</a></li>
            {% endfor %}
        </ul>
    </nav>
</div>